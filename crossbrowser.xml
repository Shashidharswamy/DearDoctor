<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="tests" name="crossbrowser">
  <test name="test-runner-chrome">
  <parameter name="BROWSER" value="chrome"></parameter>
    <classes>
      <class name="com.DearDoctorPractice.EditMyprofileTest"/>
      <class name="com.DearDoctorPractice.BookAppointmentTest"/>
      <class name="com.DearDoctorPractice.AddDoctor"/>
      <class name="com.DearDoctorPractice.ViewAppointmentHistoryTest"/>
    </classes>
  </test> <!-- Test -->
  <test name="test-runner-firefox">
  <parameter name="BROWSER" value="firefox"></parameter>
    <classes>
      <class name="com.DearDoctorPractice.EditMyprofileTest"/>
      <class name="com.DearDoctorPractice.BookAppointmentTest"/>
      <class name="com.DearDoctorPractice.AddDoctor"/>
      <class name="com.DearDoctorPractice.ViewAppointmentHistoryTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- crossbrowser -->
